{
  "redis": {
    "module": "redis",
    "connection": {
      "host": "$REDIS_HOST",
      "port": "$REDIS_PORT",
      "password": "$REDIS_PASSWORD",
      "tls" {
        "servername": "$REDIS_HOST"
      }
    }
  },
  "isDevTLS": false,
  "tls": {
    "*": {
      "pemFile": "$PATH_TO_STAR_PEM",
      "keyFile": "$PATH_TO_STAR_KEY"
    },
    "guests": {
      "pemFile": "$PATH_TO_GUESTS_PEM",
      "keyFile": "$PATH_TO_GUESTS_KEY"
    }

  },
  "auth": {
    "secret": "$JWT_PASSWORD",
    "options": {
      "algorithm": "HS256"
    }
  },
  "db": {
    "host": "$POSTGRES_HOST",
    "user": "$POSTGRES_USER",
    "password": "$POSTGRES_PASSWORD",
    "database": "$POSTGRES_DB"
  },
  "server": {
    "port": 8003,
    "routes": {
      "cors": true
    }
  },
  "talky": {
    "admins": {
      "admin": "$ADMIN_PASSWORD"
    },
    "license": "$TALKY_LICENSE",
    "apiKey": "$TALKY_API_KEY",

    "domains": {
      "api": "$API_DOMAIN",
      "guests": "$GUESTS_DOMAIN",
      "rooms": "$ROOMS_DOMAIN"
    },

    "ice": {
      "secret": "$TURN_SECRET",
      "server": "$ICE_URL"
    },

    "metrics": {
      "maskRoomNames": true
    },

    "screensharingExtensions": {
      "chrome": "$CHROME_EXTENSION_ID"
    }
  }
}
